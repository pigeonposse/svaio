{
	"name": "@svaio/unocss",
	"version": "0.0.5",
	"description": "Unocss for Sveltekit with multiple Utilities",
	"keywords": [
		"css",
		"pigeonposse",
		"pp",
		"sv",
		"svelte",
		"sveltekit",
		"toolkit",
		"unocss"
	],
	"homepage": "https://svaio.pigeonposse.com/guide/unocss",
	"bugs": {
		"url": "https://github.com/pigeonposse/svaio/issues",
		"email": "dev@pigeonposse.com"
	},
	"repository": {
		"type": "https",
		"url": "https://github.com/pigeonposse/svaio/",
		"directory": "packages/unocss"
	},
	"funding": {
		"type": "individual",
		"url": "https://pigeonposse.com/contribute"
	},
	"license": "GPL-3.0",
	"author": {
		"name": "Angelo",
		"email": "angelo@pigeonposse.com",
		"url": "https://github.com/angelespejo"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs"
		},
		"./svelte": {
			"types": "./dist/svelte/index.d.ts",
			"svelte": "./dist/svelte/index.js",
			"default": "./dist/svelte/index.js"
		},
		"./utils": {
			"types": "./dist/utils.d.ts",
			"import": "./dist/utils.mjs"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm build-lib && pnpm build-svelte",
		"build-lib": "unbuild",
		"build-svelte": "svelte-package -i ./svelte -o ./dist/svelte"
	},
	"dependencies": {
		"@iconify/tools": "4.1.2",
		"@iconify/utils": "2.3.0",
		"@unocss/extractor-svelte": "66.1.3",
		"@unocss/reset": "66.1.3",
		"unocss": "66.1.3"
	},
	"devDependencies": {
		"@svaio/repo-config": "workspace:*"
	},
	"peerDependencies": {
		"svelte": "^5",
		"vite": ">=2.0.0"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
