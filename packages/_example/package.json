{
	"name": "@svaio_/example",
	"version": "0.0.3",
	"private": true,
	"description": "Example for SvelteKit & svaio",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"perf": "pnpx unlighthouse --site http:/localhpost:1312",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview --port 1312"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "5.2.6",
		"@neoconfetti/svelte": "2.2.2",
		"@svaio/i18n": "workspace:*",
		"@svaio/media": "workspace:*",
		"@svaio/meta": "workspace:*",
		"@svaio/pwa": "workspace:*",
		"@svaio/sitemap": "workspace:*",
		"@svaio/unocss": "workspace:*",
		"svaio": "workspace:*"
	}
}
